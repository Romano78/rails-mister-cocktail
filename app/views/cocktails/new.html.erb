<div class="cocktail-form-wrapper">
    <div class="cocktail-form-header">
        <h2>Form</h2>
    </div>
    <div class="cocktail-form-container">
        <%= simple_form_for  @cocktail do |f| %>
            <%= f.input :name %>
            <%= f.input :photo, as: :file %>
             <button class="upload-button upload-button--pan">
                <%= f.submit "Submit Cocktail", class: 'submit-input-review'%>
            </button>
        <% end %>
    </div>
</div>

