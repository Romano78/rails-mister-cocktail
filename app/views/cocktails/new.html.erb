<div class="cocktail-form-wrapper">
    <div class="cocktail-form-header">
        <h2>Form</h2>
    </div>
    <div class="cocktail-form-container">
        <%= simple_form_for  @cocktail do |f| %>
            <%= f.input :name %>
            <%= f.input :photo, as: :file %>
            <%= f.submit "Submit Cocktail", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>

